        <div class="main-panel">
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-header card-header-primary">
                                    <h4 class="card-title">Add Customer</h4>
                                    <p class="card-category"></p>
                                </div>
                                <div class="card-body">
                                    <form>
                                        <div class="row">

                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Customer Name</label>
                                                    <input id="txtCustomerName" type="text" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Customer Address</label>
                                                    <textarea id="txtCustomerAddress" type="text" class="form-control"></textarea>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Customer DOB</label>
                                                    <input id="txtDOB" type="date" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Field Area (Acre) </label>
                                                    <input id="txtFieldArea" type="text" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Contact Number</label>
                                                    <input id="txtContactNumber" type="text" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Contact Email</label>
                                                    <input id="txtEmail" type="text" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Aadhar Number</label>
                                                    <input id="txtAadharNumber" type="text" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Bank Account Number</label>
                                                    <input id="txtBankAccountnumber" type="text" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label class="bmd-label-floating">Photo</label>
                                                    <input id="file1" type="file" />
                                                </div>
                                            </div>
                                        </div>

                                        <button type="submit" class="btn btn-primary pull-right" onclick="saveContact()">Submit</button>
                                        <div class="clearfix"></div>
                                        <hr>
                                    </form>
                                    <div class="col-md-12">
                                        <div class="tableFixHead">
                                            <table id="tblContact" class="table table-bordered table-sm">
                                                <thead style="background-color:#ab47bc;border-width:2px;">
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>CustomerName</th>
                                                        <th>Customer Address</th>
                                                        <th>Customer DOB</th>
                                                        <th>Field Area</th>
                                                        <th>Contact Number</th>
                                                        <th>Contact Email</th>
                                                        <th>Adhar Number</th>
                                                        <th>Bank Account Number</th>
                                                        <th>Photo</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>













